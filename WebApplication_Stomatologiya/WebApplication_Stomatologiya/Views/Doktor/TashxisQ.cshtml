@{
    ViewBag.Title = "Saqlash";
}
<section>
    <div class="modal-body">
        <div class="loginf_module">
            <div class="module form-module">
                <div class="toggle">
                    <i class="fa fa-times fa-pencil"></i>

                </div>
                <div class="form">
                    <h3>Avtomatlashtirilgan anketa</h3>                    
                    <form action="/Doktor/TashxisQ" method="post">
                        <input type="text" name="Tashxis" placeholder="Tashxis" required="">
                        <input type="text" name="Shikoyat" placeholder="Shikoyat" required="">
                        <input type="text" name="KasRivoj" placeholder="Kasallik Rivojlanishi" required="">                      
                        <input type="text" name="LabNatija" placeholder="Labaratoriya natijasi" required="">
                        <input type="text" name="Tishlash" placeholder="Tishlash" required="">
                        <input type="text" name="OgizBosh" placeholder="Og'iz bo'shlig'i holati" required="">
                        <input type="text" name="Summa" placeholder="Davolanish summasi" required="">
                        <input type="text" name="DoktorId" placeholder="DoktorId" required="" value="@ViewBag.d" hidden="hidden">
                        <input type="text" name="BemorId" placeholder="BemorId" required="" value="@ViewBag.b"  hidden="hidden">
                        <input type="submit" value="Saqlash">
                    </form>

                </div>


            </div>
        </div>
    </div>
</section>